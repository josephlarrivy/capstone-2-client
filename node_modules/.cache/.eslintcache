vigate(-1)\n  }\n  \n\n  if (token === undefined || token === null) {\n    return (\n      <div className='NavBar'>\n        <Navbar>\n          <Nav >\n            <button onClick={goBack}>Back</button>\n            <NavItem >\n              <NavLink to=\"/\">Home</NavLink>\n            </NavItem>\n            <NavItem >\n              <NavLink to=\"/map\">Map</NavLink>\n            </NavItem>\n            {/* <NavItem >\n              <NavLink to=\"/gallery\">Photo Gallery</NavLink>\n            </NavItem> */}\n            <NavItem >\n              <NavLink to=\"/register\">Register</NavLink>\n            </NavItem>\n            <NavItem>\n              <NavLink to=\"/login\">Log In</NavLink>\n            </NavItem>\n          </Nav>\n        </Navbar>\n      </div>\n    )\n  } else {\n    return (\n      <div className='NavBar'>\n        <Navbar>\n          <Nav>\n            <button onClick={goBack}>Back</button>\n            <NavItem>\n              <NavLink to=\"/\">Home</NavLink>\n            </NavItem>\n            <NavItem >\n              <NavLink to=\"/map\">Map</NavLink>\n            </NavItem>\n            <NavItem >\n              <NavLink to=\"/myTrips\">My Trips</NavLink>\n            </NavItem>\n            <NavItem>\n              <NavLink onClick={logOut} id=\"nav-logout\">Log Out</NavLink>\n            </NavItem>\n            {/* <NavItem >\n              <NavLink to=\"/gallery\">Photo Gallery</NavLink>\n            </NavItem> */}\n            {/* <NavItem >\n              <NavLink to=\"/activities\">Activities</NavLink>\n            </NavItem> */}\n          </Nav>\n        </Navbar>\n      </div>\n    )\n  }\n  \n}\n\nexport default NavBar;","/Users/josephlarrivy/software-engineering/springboard/assignments/capstone-2-rework/client/src/apis/nationalParksApi.js",[],[],"/Users/josephlarrivy/software-engineering/springboard/assignments/capstone-2-rework/client/src/components/ThingsToDo.js",["382","383","384","385","386","387","388","389","390"],[]
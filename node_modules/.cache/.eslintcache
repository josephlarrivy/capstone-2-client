)\n  }, [])\n\n  return (\n    <div id=\"visitor-centers-main-container\">\n      <NavBar\n        token={token}\n        setToken={setToken}\n      />\n      <div id=\"visitor-centers-container\">\n        <h1>Visitor Centers in {stateName}</h1>\n        <div id='visitor-centers-items'>\n          {data && data.map(item => {\n            return (\n              <div key={item.id} className=\"visitor-centers-item\">\n                <div className=\"visitor-centers-item-image\">\n                  {item.images.length > 0\n                    ? <div className=\"visitor-centers-item-image-container\">\n                        <img src={item.images[0].url}></img>\n                      </div>\n                    : <div className=\"visitor-centers-item-image-container\">\n                        <img src={require('../images/black.png')}></img>\n                        {/* <p>no image</p> */}\n                    </div>\n                  }\n                </div>\n                <div className=\"visitor-centers-item-header\">\n                  <p className=\"visitor-center-name\">{item.name}</p>\n\n\n                  <div id=\"buttons-sub-container\">\n\n                    <button \n                      onClick={() => navigate(`/park/${item.parkCode}`)}>\n                      Go To Park Details\n                    </button>\n                  \n                  \n                    {/* <AddToTripDropdown\n                      type='visitorCenter'\n                      route={`/visitorcenters/${USstate}`}\n                      name={item.name}\n                      description={item.description}\n                      parkcode={item.parkCode}\n                      latitude={item.latitude}\n                      longitude={item.longitude}\n                    /> */}\n                  </div>\n\n\n\n                  {item.addresses.length > 0\n                    ? <p>{item.addresses[0].line1} {item.addresses[0].city}, {item.addresses[0].stateCode} {item.addresses[0].postalCode}</p>\n                    : <p></p>\n                  }\n                  <p className=\"visitor-center-description\">{item.description}</p>\n                </div>\n                <div className=\"visitor-centers-item-info\">  \n                  {item.contacts.emailAddresses.length > 0\n                    ? <p>email: {item.contacts.emailAddresses[0].emailAddress}</p>\n                    : <p></p>\n                  }\n                  {item.contacts.phoneNumbers.length > 0\n                    ? <p>phone: {item.contacts.phoneNumbers[0].phoneNumber}</p>\n                    : <p></p>\n                  }\n                </div>\n                {/* <div className=\"operating-hours\">\n                  {item.operatingHours.length > 0\n                    ? <p>operating hours</p>\n                    : <p>hours not given</p>\n                  }\n                </div> */}\n              </div>\n            )\n          })}\n        </div>\n        <br></br>\n        <br></br>\n        <br></br>\n      </div>\n    </div>\n  )\n}\n\nexport default VisitorCenters;","/Users/josephlarrivy/software-engineering/springboard/assignments/capstone-2-rework/client/src/components/ArticlesSearchResults.js",["320","321","322","323"],[],"import React, { useState, useEffect } from \"react\";\nimport { useParams, useNavigate } from \"react-router-dom\";\nimport NParksServiceRequest from \"../apis/nationalParksApi\";\nimport NavBar from \"../NavBar\";\nimport '../css/ArticlesSearchResults.css'\n\nconst ArticlesSearchResults = ({ token, setToken }) => {\n\n  const { searchTerm } = useParams()\n  const [numResults, setNumResults] = useState(10)\n  const [results, setResults] = useState(null)\n  const navigate = useNavigate();\n\n\n  useEffect(() => {\n    const doSearch = async () => {\n      const response = await NParksServiceRequest.searchInArticles(searchTerm, numResults)\n      console.log(response)\n      setResults(response)\n    }\n    doSearch()\n  }, [numResults])\n\n  const increaseNumberOfSearchResults = () => {\n    setNumResults(numResults + 10)\n  }\n\n  const separateArray = (arr) => {\n    return arr.join(\", \");\n  }\n\n  const createParkLink = (arr) => {\n    const buttons = []\n    for (let item of arr) {\n      buttons.push(<button onClick={() => navigate(`/park/${item.parkCode}`)}>{item.name}</button>)\n    }\n    return buttons\n  }\n\n  return (\n    <div id=\"articles-search-results-main-container\">\n      <NavBar\n        token={token}\n        setToken={setToken}\n      />\n      \n      <div id=\"articles-main-container\">\n        <h1>Article Search Results</h1>\n        {results && results.map(item => {\n          return (\n            <>\n              <div className=\"articles-item-container\">\n                {item.listingImage.url\n                  ? <div className=\"articles-item-image-container\">\n                    <img src={item.listingImage.url}></img>\n                  </div>\n                  : <div className=\"articles-item-image-container\">\n                    <img src={require('../images/black.png')}></img>\n                  </div>\n                }\n                <div className=\"articles-item-container-info\">\n                  <a href={item.url} target=\"_blank\"><h4>{item.title}</h4></a>\n                  {item.tags \n                    ? <p><b>Interests:</b> {separateArray(item.tags)}</p>\n                    : <></>\n                  }\n                  <p>{item.listingDescription}</p>\n                  {item.relatedParks.length>0\n                    ? <div className=\"related-parks-buttons\">\n                        <b>Related Parks: </b>{createParkLink(item.relatedParks)}\n                      </div>\n                    : <></>\n                  }\n                </div>\n              </div>\n            </>\n\n          )\n        })}\n      </div>\n      <button className=\"articles-load-more-search-results-button\" onClick={increaseNumberOfSearchResults}>More Articles about '{searchTerm}'</button>\n    </div>\n  )\n}\n\nexport default ArticlesSearchResults;","/Users/josephlarrivy/software-engineering/springboard/assignments/capstone-2-rework/client/src/components/TourDetails.js",["324","325","326","327","328"],[],"import React, { useState, useEffect } from \"react\";\nimport { useParams, useNavigate } from \"react-router-dom\";\nimport NParksServiceRequest from \"../apis/nationalParksApi\";\nimport NavBar from \"../NavBar\";\nimport Weather from \"./Weather\";\nimport MiniMap from \"./MiniMap\";\nimport Sun from \"./Sun\";\nimport '../css/TourDetails.css'\nimport Loading from \"./Loading\";\nimport AudioPlayer from \"./AudioPlayer\";\nimport useStateNameConverter from \"../hooks/useStateNameConverter\";\nimport AddToTripDropdown from \"./AddToTripDropdown\";\n\nconst TourDetails = ({token, setToken}) => {\n\n  const { id } = useParams()\n  const [data, setData] = useState(null)\n  const [zoom, setZoom] = useState(8)\n  const [centerPosition, setCenterPosition] = useState(null)\n  const [stateName, convertStateName] = useStateNameConverter();\n  const navigate = useNavigate();\n\n\n  useEffect(() => {\n    const getData = async () => {\n\n      const tourData = await NParksServiceRequest.getTourDetails(id)\n      console.log(tourData)\n      setData(tourData)\n\n      const location = await NParksServiceRequest.getParkLatLong(tourData.park.parkCode)\n      const parsedLat = parseFloat(location.latitude)\n      const parsedLon = parseFloat(location.longitude)\n      setCenterPosition([parsedLat, parsedLon])\n\n    }\n    getData()\n  }, [])\n\n\n  const convertDurationUnit  = (input) => {\n    if (input === 'm') {\n      return 'minutes'\n    } else if (input === 'h') {\n      return 'hours'\n    }\n  }\n\n  const commaSeparator = (string) => {\n    let abbreviation = string.split(',').map(function (item) {\n      return item.trim();\n    }).join(', ');\n    return abbreviation\n  }\n\n\n  const createParkLink = (item) => {\n    return (<button onClick={() => navigate(`/park/${item.parkCode}`)}>{item.fullName}</button>)\n  }\n\n  useEffect(() => {\n    console.log(centerPosition)\n  }, [centerPosition])\n\n\n  if (data === null) {\n    return (\n      <Loading />\n    )\n  } else {\n  return (\n    <div className=\"tour-details\">\n      <NavBar\n        token={token}\n        setToken={setToken}\n      />\n      <div className=\"tour-details-main-container\">\n        {centerPosition && \n          <>\n            <div className=\"weather\">\n              <Weather centerPosition={centerPosition} />\n            </div>\n            <div className=\"tour-details-header-map-conatiner\">\n              <MiniMap\n                zoom={zoom}\n                centerPosition={centerPosition}\n              />\n            </div>\n          <div id=\"park-options-buttons\">\n            {/* <AddToTripDropdown\n              type='tour'\n              route={`/tourDetails/${data.id}`}\n              name={data.park.fullName}\n              description={data.description}\n              parkcode={data.park.parkCode}\n              latitude={centerPosition[0].toString()}\n              longitude={centerPosition[1].toString()}\n            /> */}\n          </div>\n            <div className=\"tour-details-map-footer\">\n              <Sun centerPosition={centerPosition} />\n            </div>\n          </>\n        }\n\n        <div className=\"tour-details-header-main\">\n          <h1><b>Tour: </b>{data.title}</h1>\n          {data.park\n            ? <div className=\"related-parks-buttons\">\n              <b>Located in: </b>{createParkLink(data.park)}\n            </div>\n            : <></>\n          }\n          <p><b>States: </b>{commaSeparator(data.park.states)}</p>\n          <p><b>Duration: </b>{data.durationMin}-{data.durationMax} {convertDurationUnit(data.durationUnit)}</p>\n          <p>{data.description}</p>\n        </div>\n\n        <br></br>\n\n        <div className=\"tour-details-stops-container\">\n          <h4>Follow this step-by-step guide to make the most of you visit</h4>\n          {data.stops && data.stops.map(item => {\n            return (\n              <div key={item.ordinal} className=\"tour-details-stops-item-container\">\n                <div className=\"left\">\n                  <p><u><b>Stop {item.ordinal}</b></u></p>\n                  <p className=\"tour-details-stops-item-container-name\">{item.assetName}</p>\n                  {item.audioFileUrl\n                    ? <p>Click to play audio</p>\n                    : <p>No audio for this stop</p>\n                  }\n                  <AudioPlayer src={item.audioFileUrl} />\n                </div>\n                <div className=\"right\">\n                  <p><b>Type: </b>{item.assetType}</p>\n                  <p><b>Significance: </b>{item.significance}</p>\n                  <p><b>Directions to next stop: </b>{item.directionsToNextStop}</p>\n                </div>\n              </div>\n            )\n            })\n          }\n        </div>\n      </div>\n\n\n      <br></br>\n      <br></br>\n      <br></br>\n      <br></br>\n\n    </div>\n  )\n}\n}\n\nexport default TourDetails;","/Users/josephlarrivy/software-engineering/springboard/assignments/capstone-2-rework/client/src/components/Park.js",["329","330","331","332"],[],"/Users/josephlarrivy/software-engineering/springboard/assignments/capstone-2-rework/client/src/components/Home.js",["333","334","335","336"],[],"import React, { useState, useEffect } from \"react\";\nimport NavBar from \"../NavBar\";\n\nimport '../css/Home.css'\nimport Banner from \"./Banner\";\nimport NewsReleases from \"./NewsReleases\";\nimport Alerts from \"./Alerts\";\nimport VisitorCentersBanner from \"./VisitorCentersBanner\";\nimport ParksByStateBanner from \"./ParksByStateBanner\";\nimport { Button } from \"reactstrap\";\nimport SearchBanner from \"./SearchBanner\";\nimport PlanATripBanner from \"./PlanATripBanner\";\n\nconst Home = ({ token, setToken }) => {\n  \n\n  return (\n    <div className=\"home-container\">\n      <NavBar\n        token={token}\n        setToken={setToken}\n      />\n      <div className=\"home-main\">\n        {/* <PlanATripBanner />\n        <br></br>\n        <br></br> */}\n        <SearchBanner />\n        <br></br>\n        <br></br>\n        <NewsReleases />\n        <br></br>\n        <br></br>\n        <Banner />\n        <br></br>\n        <br></br>\n        \n        <ParksByStateBanner />\n        <br></br>\n        <br></br>\n        <Alerts />\n        <br></br>\n        <br></br>\n        <VisitorCentersBanner />\n        <br></br>\n        <br></br>\n        <br></br>\n        <br></br>\n      </div>\n    </div>\n  )\n}\n\nexport default Home;","/Users/josephlarrivy/software-engineering/springboard/assignments/capstone-2-rework/client/src/components/Map.js",[],[],"/Users/josephlarrivy/software-engineering/springboard/assignments/capstone-2-rework/client/src/components/Book.js",["337","338"],[],"import React, { useState } from \"react\";\nimport NavBar from \"../NavBar\";\nimport RecreationAPI from \"../apis/RecreationApi\";\n\nconst Book = ({ token, setToken }) => {\n  \n  return (\n    <div id=\"book-container\">\n      <NavBar token={token} setToken={setToken} />\n      <h1>Book</h1>\n    </div>\n  );\n};\n\nexport default Book;\n","/Users/josephlarrivy/software-engineering/springboard/assignments/capstone-2-rework/client/src/components/MyTrips.js",["339","340","341","342","343","344"],[],"import React, {useState, useEffect} from \"react\";\nimport NavBar from \"../NavBar\";\nimport useLocalStorage from \"../hooks/useLocalStorage\";\nimport TripNameForm from \"../forms/TripNameForm\";\nimport BackendApiRequest from \"../apis/backendApi\";\nimport { Link, useNavigate } from \"react-router-dom\";\n\nimport '../css/MyTrips.css'\n\nconst MyTrips = ({token, setToken}) => {\n\n  const [localStoreToken, localRemoveToken, localRetrieveToken, localDecodeToken] = useLocalStorage()\n  const [username, setUsername] = useState(null)\n  const [trips, setTrips] = useState(null)\n  const navigate = useNavigate()\n\n  const getTrips = async () => {\n    const data = await BackendApiRequest.getUserTrips(username)\n    console.log(data)\n    setTrips(data)\n  }\n  \n  useEffect(() => {\n    const getUsername = async () => {\n      const token = await localDecodeToken()\n      setUsername(token.username)\n    }\n    getUsername()\n  }, [])\n  \n  useEffect(() => {\n    if (username) {\n      getTrips()\n    }\n  }, [username])\n\n  const deleteTrip = async (tripname) => {\n    const resp = await BackendApiRequest.deleteTrip(tripname)\n    if (resp.status === 202) {\n      const getTrips = async () => {\n        const data = await BackendApiRequest.getUserTrips(username)\n        console.log(data)\n        setTrips(data)\n      }\n      getTrips()\n    }\n  }\n\n  const viewTripDetails = (tripname) => {\n    navigate(`/tripDetails/${tripname}`)\n  }\n\n\n  return (\n    <div id=\"my-trips-main-container\">\n      <NavBar \n        token={token}\n        setToken={setToken}\n      />\n      <div id=\"my-trips-page-container\">\n        <h1>My Trips</h1>\n        <div id=\"trip-name-form\">\n          <TripNameForm getTrips={getTrips}/>\n        </div>\n        <div id=\"trip-list-container\">\n          {trips && trips.map(trip => {\n            return (\n              <div key={trip.tripname} className=\"trip-container\">\n                  <div className=\"trip-name-container\">\n                    <h4>{trip.tripname}</h4>\n                  </div>\n                  <div className=\"buttons-container\">\n                    <button onClick={() => { viewTripDetails(`${trip.id}/${trip.tripname}`) }}>view trip</button>\n                    <button onClick={() => { deleteTrip(trip.tripname) }}>delete trip</button>\n                  </div>\n              </div>\n            )\n          })\n\n          }\n
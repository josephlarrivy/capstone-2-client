 {\n    console.log(event.target.value)\n    setSelectedPark(event.target.value)\n  }\n\n  const handleGoToPark = async (e) => {\n    navigate(`/park/${selectedPark}`)\n  }\n\n  return (\n    <div id='parks-by-state-container'>\n      {USstate === null\n        ? <h1>Find a Park</h1>\n        : <h1>Parks in {stateName}</h1>\n      }\n\n      <div id=\"state-select\">\n        <StateSelect USstate={USstate} setUSstate={setUSstate} />\n      </div>\n\n      {parksList && \n        <form>\n          <label htmlFor=\"park-select\"></label>\n          <select id=\"park-select-dropdown\" name=\"park-select\" onChange={handleParkSelect}>\n            {parksList.map(item => {\n              return (\n                <option\n                  key={item.parkCode}\n                  value={item.parkCode}\n                  // onClick={handleParkSelect}\n                  >{item.name}\n                </option>\n              )\n            })}\n          </select>\n        </form>\n      }\n\n      {selectedPark === null\n        ? <p></p>\n        : <div\n            id=\"search-button\"\n            onClick={(e) => handleGoToPark(e)}\n          ><p>View Park</p>\n          </div>\n      }\n      \n\n\n    </div>\n  )\n}\n\nexport default ParksByStateBanner;","/Users/josephlarrivy/software-engineering/springboard/assignments/capstone-2-rework/client/src/components/Banner.js",["438","439"],[],"import React, { useState, useEffect } from \"react\";\nimport NParksServiceRequest from \"../apis/nationalParksApi\";\nimport '../css/Banner.css'\nimport StateSelect from \"./StateSelect\";\nimport { useNavigate } from 'react-router-dom';\nimport useStateNameConverter from \"../hooks/useStateNameConverter\";\n\n\nconst Banner = () => {\n\n  const [USstate, setUSstate] = useState('placeholder')\n  const [stateName, convertStateName] = useStateNameConverter();\n  const navigate = useNavigate();\n\n  useEffect(() => {\n    convertStateName(USstate)\n  }, [USstate])\n\n  const handleGuidedSearch = async (e, type) => {\n    if (USstate === 'placeholder') {\n      // setUSstate('Pick a state to search')\n    } else {\n      navigate(`/supplemental/${type}/${USstate}`);\n    }\n  }\n\n  return (\n    <div id=\"banner-one-container\">\n      <h1>Discover Events, Articles, and Things to do</h1>\n      <div id=\"state-select\">\n        <StateSelect USstate={USstate} setUSstate={setUSstate}/>\n      </div>\n      {USstate === 'placeholder'\n        ? <></>\n        : <div id=\"banner-one-buttons\">\n          <div\n            id=\"banner-one-div-one\"\n            onClick={(e) => handleGuidedSearch(e, 'events')}>\n            <p>Events in {stateName}</p>\n          </div>\n          <div\n            id=\"banner-one-div-two\"\n            onClick={(e) => handleGuidedSearch(e, 'articles')}>\n            <p>Articles about {stateName}</p>\n          </div>\n          <div\n            id=\"banner-one-div-three\"\n            onClick={(e) => handleGuidedSearch(e, 'thingstodo')}>\n            <p>Things To Do in {stateName}</p>\n          </div>\n        </div>\n      }\n      \n    </div>\n  )\n}\n\nexport default Banner;","/Users/josephlarrivy/software-engineering/springboard/assignments/capstone-2-rework/client/src/components/Alerts.js",["440","441","442","443"],[],"import React, { useEffect, useState } from \"react\";\nimport NParksServiceRequest from \"../apis/nationalParksApi\";\n\nimport '../css/Alerts.css'\n\nconst Alerts = () => {\n\n  const [alerts, setAlerts] = useState(null)\n  const [numAlerts, setNumAlerts] = useState(6)\n  const [alertDetails, setAlertDetails] = useState('Hover over an alert to see full description')\n  const [alertStyle, setAlertStyle] = useState({ border: \"2px solid white\" })\n  const [parkName, setParkName] = useState(null)\n\n  useEffect(() => {\n    const getData = async () => {\n      const response = await NParksServiceRequest.getAlerts(numAlerts)\n      // console.log(response)\n      setAlerts(response)\n    }\n    getData()\n  }, [numAlerts])\n\n  \n  function formatDate(dateString) {\n    const date = new Date(dateString);\n    const options = {\n      // year: 'numeric',\n      month: 'long',\n      day: 'numeric',\n      hour: 'numeric',\n      minute: 'numeric',\n      // second: 'numeric'\n    };\n    return date.toLocaleString('en-US', options);\n  }\n\n\n  function convertAlertType(input) {\n    if (input === 'Park Closure') {\n      return 'alert-type-closure'\n    } else if (input === 'Information') {\n      return 'alert-type-information'\n    } else if (input === 'Caution') {\n      return 'alert-type-caution'\n    } else if (input === 'Danger') {\n      return 'alert-type-danger'\n    }\n  }\n\n  const getMoreAlerts = () => {\n    setNumAlerts(numAlerts+6)\n    // console.log('get more alerts')\n  }\n\n  const showAlertDetails = (e, input) => {\n    setAlertStyle({ border: \"2px solid white\" })\n    setAlertDetails(input)\n  }\n  const hideAlertDetails = () => {\n    setAlertDetails('Hover over an alert to see full description')\n    setAlertStyle({ border: \"2px solid white\" })\n  }\n  \n\n  return (\n    <div id=\"alerts-container\">\n      <h1>Park Alerts</h1>\n      <div className=\"alert-details\" style={alertStyle}>\n        <p>{alertDetails}</p>\n      </div>\n      <div id=\"alerts-items\">\n        {alerts && alerts.map(item => {\n\n          let forMattedDate = formatDate(item.lastIndexedDate)\n          let convertedAlertType = convertAlertType(item.category)\n          \n          return (\n            <div \n              key={item.id}\n              className=\"alert-item\"\n              onMouseEnter={(e) => { showAlertDetails(e, item.description)}}\n              onMouseLeave={hideAlertDetails}>\n              <div className={convertedAlertType}>\n                <p>{item.category}</p>\n              </div>\n              <div className=\"alert-information\">\n                <p>{forMattedDate}</p>\n                <p>{item.title}</p>\n              </div>\n              <div className=\"more-info-icon\">\n                {item.url\n                  ? <a href={item.url} target=\"_blank\">\n                      <img className=\"more-info-icon\" src={require('../images/more-info-icon.png')}></img>\n                    </a>\n                  : <></>\n                }\n              </div>\n            </div>\n          )\n        })\n        }\n      </div>\n      <button id='load-more-home-button-alerts' onClick={getMoreAlerts}>See More Alerts</button>\n    </div>\n  )\n}\n\nexport default Alerts;","/Users/josephlarrivy/software-engineering/springboard/assignments/capstone-2-rework/client/src/components/MainMap.js",["444","445","446","447","448"],[],"import React, { useState, useEffect } from 'react'\nimport { Link } from 'react-router-dom';\n\n\nimport { MapContainer, Marker, Popup, TileLayer } from 'react-leaflet'\n\nimport '../css/MainMap.css'\nimport NavBar from '../NavBar';\n\nconst MainMap = ({showingParks}) => {\n\n  const [zoom, setZoom] = useState(4)\n  const [centerPosition, setCenterPosition] = useState([35, -94])\n\n  return (\n    <div className='map-container'>\n      <MapContainer\n        key={'MapContainer'}\n        center={centerPosition}\n        zoom={zoom}>\n        \n        <TileLayer\n          key={'tileLayer'}\n          attribution='&copy; <a href=\"https://www.openstreetmap.org/copyright\">OpenStreetMap</a> contributors'\n          url=\"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\"\n        />\n\n\n\n        {showingParks &&\n          showingParks.map(park => {\n            return (\n              <Marker\n                key={park.parkCode}\n                position={[\n                  park.latitude,\n                  park.longitude\n                ]}\n              >\n                <Popup>\n                  <h4>{park.fullName}</h4>\n                  \n\n                  <div className='popup-image-div'>\n                    <img\n                      className='popup-image'\n                      src={park.images[0].url}\n                    ></img>\n                  </div>\n\n\n                  <div className='popup-description-div'>\n                    <p><Link\n                      className='link-info'\n                      to={`/park/${park.parkCode}`\n                      }>View Park\n                    </Link></p>\n                    <p className='popup-description'>{park.description}</p>\n                  </div>\n                </Popup>\n              </Marker>\n            )\n          })\n        }\n\n\n\n      </MapContainer>\n    </div>\n  )\n}\n\n\nexport default MainMap;","/Users/josephlarrivy/software-engineering/springboard/assignments/capstone-2-rework/client/src/keys.js",[],[],"/Users/josephlarrivy/software-engineering/springboard/assignments/capstone-2-rework/client/src/apis/weatherApi.js",[],[],"/Users/josephlarrivy/software-engineering/springboard/assignments/capstone-2-rework/client/src/apis/sunriseSunsetApi.js",[],[],"/Users/josephlarrivy/software-engineering/springboard/assignments/capstone-2-rework/client/src/components/StateSelect.js",[],[],{"ruleId":"449","severity":1,"message":"450","line":1,"column":8,"nodeType":"451","messageId":"452","endLine":1,"endColumn":12},{"ruleId":"449","severity":1,"message":"453","line":13,"column":10,"nodeType":"451","messageId":"452","endLine":13,"endColumn":25},{"ruleId":"449","severity":1,"message":"454","line":13,"column":27,"nodeType":"451","messageId":"452","endLine":13,"endColumn":43},{"ruleId":"449","severity":1,"message":"455","line":13,"column":65,"nodeType":"451","messageId":"452","endLine":13,"endColumn":81},{"ruleId":"456","severity":1,"message":"457","line":19,"column":6,"nodeType":"458","endLine":19,"endColumn":8,"suggestions":"459"},{"ruleId":"449","severity":1,"message":"460","line":21,"column":10,"nodeType":"451","messageId":"452","endLine":21,"endColumn":18},{"ruleId":"449","severity":1,"message":"461","line":21,"column":20,"nodeType":